{
    "bindings": [
        {
            "authLevel": "anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": [
                "post"
            ],
            "route": "user"
        },
        {
            "type": "http",
            "direction": "out",
            "name": "$return"
        },
        {
            "type": "cosmosDB",
            "direction": "out",
            "name": "doc",
            "connectionStringSetting": "CosmosDB",
            "databaseName": "bayrou",
            "containerName": "users",
            "createIfNotExists": true
        }
    ]
}